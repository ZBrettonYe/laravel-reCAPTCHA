{
  "name": "zbrettonye/no-captcha",
  "description": "No CAPTCHA reCAPTCHA For Laravel6~8",
  "keywords": [
    "recaptcha",
    "no-captcha",
    "captcha",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zbrettonye",
      "email": "monkeyblacktech97@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2|^8",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.2|^7",
    "illuminate/support": "^6|^7|^8|^9"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "ZBrettonYe\\NoCaptcha\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZBrettonYe\\NoCaptcha\\NoCaptchaServiceProvider"
      ],
      "aliases": {
        "NoCaptcha": "ZBrettonYe\\NoCaptcha\\Facades\\NoCaptcha"
      }
    }
  }
}
