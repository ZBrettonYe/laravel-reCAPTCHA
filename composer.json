{
    "name": "misechow/no-captcha",
    "description": "No CAPTCHA reCAPTCHA For Laravel.",
    "keywords": ["misechow", "recaptcha", "no-captcha", "captcha", "laravel", "laravel4", "laravel5"],
    "license": "MIT",
    "authors": [
        {
            "name": "misechow",
            "email": "mise3uc@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Misechow\\NoCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Misechow\\NoCaptcha\\NoCaptchaServiceProvider"
            ],
            "aliases": {
                "NoCaptcha": "Misechow\\NoCaptcha\\Facades\\NoCaptcha"
            }
        }
    }
}
