{
    "name": "zbrettonye/no-captcha",
    "description": "No CAPTCHA reCAPTCHA For Laravel.",
    "keywords": ["recaptcha", "no-captcha", "captcha", "laravel", "laravel4", "laravel5", "laravel6"],
    "license": "MIT",
    "authors": [
        {
            "name": "zbrettonye",
            "email": "monkeyblacktech97@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "^5.0|^6.0|^7.0",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "ZBrettonYe\\NoCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZBrettonYe\\NoCaptcha\\NoCaptchaServiceProvider"
            ],
            "aliases": {
                "NoCaptcha": "ZBrettonYe\\NoCaptcha\\Facades\\NoCaptcha"
            }
        }
    }
}
